# Copy this file to .env and fill in the values before starting the server.
# Do NOT commit your real .env to source control. This is an example only.

# MongoDB connection string
MONGODB_URI=mongodb://localhost:27017/stock

# Server port
PORT=5001

# JWT secret used for signing tokens
JWT_SECRET=your_jwt_secret_here

# Alpha Vantage API key (required for international stocks / company info)
# Get a free key from https://www.alphavantage.co/support/#api-key
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_api_key_here
    